                if($('.popular_categories_prev').length)
                {
                    var prev = $('.popular_categories_prev');
                    prev.on('click', function()
                    {
                        popularSlider.trigger('prev.owl.carousel');
                    });
                }

                if($('.popular_categories_next').length)
                {
                    var next = $('.popular_categories_next');
